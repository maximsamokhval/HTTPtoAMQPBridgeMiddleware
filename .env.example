# RabbitMQ Connection
RABBITMQ_URL=amqp://guest:guest@localhost:5672/edi

# Connection Settings
RABBITMQ_PREFETCH_COUNT=10
RETRY_ATTEMPTS=5
RETRY_BASE_DELAY=1.0
PUBLISH_TIMEOUT=30.0
CONSUME_TIMEOUT=30.0

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Application
APP_NAME=rmq-middleware
APP_HOST=0.0.0.0
APP_PORT=8000
